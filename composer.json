{
    "name": "analogue/presenter",
    "description": "Easy view presenters for Analogue ORM",
    "keywords": [
        "orm",
        "datamapper",
        "laravel",
        "entity",
        "repository",
        "mapper",
        "presenter",
        "view-models"
    ],
    "homepage": "https://github.com/analogueorm/presenter",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Wales",
            "email": "mark@smallhadroncollider.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "analogue/orm": ">=5.1",
        "nesbot/carbon": "~1.20"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Analogue\\Presenter\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
